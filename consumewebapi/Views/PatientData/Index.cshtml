@model IEnumerable<ConsumeWebAPI.Models.PatientDataModel>

@{
    ViewBag.Title = "Manipulate Server Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Consume Asp.net WebAPI :: Server Data</h2>

<article>
    <p>
        Here you can manipulat server data.
    </p>

    <p>
        @Html.ActionLink("Create New", "Create", null, new { @class = "actionlinkbutton" })
    </p>

    <p>
        <img src="PatientData/ChartArrayBasic"/>
    </p>

    <div>
        <table>

            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DoctorID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StartDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EndDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Type)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Time)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RecordID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RecordName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayNameFor(model => model.DoctorID)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.Name)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.StartDate)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.EndDate)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.Type)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.Time)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.RecordID)
                    </td>
                    <td>
                        @Html.DisplayNameFor(model => model.RecordName)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                    </td>
                </tr>
            }

        </table>
    </div>
</article>

<aside>
    <h3>Navigation</h3>
    <p>
        From here you can navigate other parts of app.
    </p>
    <ul>
        <li>@Html.ActionLink("Home", "Index", "Home")</li>
        <li>@Html.ActionLink("Patient Data", "Index", "PatientData")</li>
        <li>@Html.ActionLink("About", "About", "Home")</li>
        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
    </ul>
</aside>
