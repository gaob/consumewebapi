@model IEnumerable<ConsumeWebAPI.Models.PatientDataModel>

@{
    ViewBag.Title = "Manipulate Server Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Doctor Requests</h2>

<article>

    <p>
        @Html.ActionLink("Create a New Request", "Create", null, new { @class = "actionlinkbutton" })
    </p>

    <div>
        <table>

            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.DoctorID)
                </th>
                <th width="150">
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StartDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EndDate)
                </th>
                <th width="100">
                    @Html.DisplayNameFor(model => model.Type)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Time)
                </th>
                <th width="100">
                    @Html.DisplayNameFor(model => model.RecordID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RecordName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.DoctorID)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.StartDate)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.EndDate)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Type)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.Time)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.RecordID)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.RecordName)
                    </td>
                </tr>
            }

        </table>
    </div>
</article>

<aside>
    <h3>Patient Information</h3>
    <ul>
        <li>Name: John Smith</li>
        <li>ID: 90000101</li>
    </ul>
</aside>
